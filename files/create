CREATE TABLE post(
	id int(11) PRIMARY KEY NOT null,
    name varchar(20) NOT null,
    year int(2),
    branch varchar(20)
);


create table entervalue(
    id int(4) primary KEY,
    name varchar(20));


  create table MBD(
name varchar(20), 
age int(2),
sex varchar(7),
index_no int(5),
CR_no int(5),
addmission_no int(5) PRIMARY KEY,
EC_no int(5),
DOA date,
DOS date,
DOD date,
address varchar(50), 
telephone int(12),
mobile int(10),
email varchar(30),
self varchar(20), 
parents varchar(20),
grandparents varchar(20), 
r_physicians varchar(20),
presenting varchar(20),
deformity varchar(20), 
n_o_fracture varchar(20),
bone_pain varchar(20),
short_stature varchar(20),  
teeth_abnormality varchar(20), 
duration varchar(20), 
ht int(2), 
wt int(2),
BMI int(2)
);

addmission_no int(5) PRIMARY KEY,
